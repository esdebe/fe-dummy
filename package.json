{
  "name": "frontend-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:ts": "eslint --ext .ts,.tsx \"./src/**/*.{ts,tsx}\"",
    "lint:style": "stylelint \"./src/**/*.css\"",
    "prettier": "prettier --write \"./src/**/*.{ts,tsx,css}\"",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "husky:add": "rimraf .husky && husky install && npx husky add .husky/pre-commit \"npm run pre-commit\"",
    "ncu": "npm-check-updates --timeout=120000"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.11",
    "@lukemorales/query-key-factory": "^1.2.0",
    "@mui/base": "^5.0.0-alpha.120",
    "@mui/utils": "^5.11.12",
    "@tanstack/react-query": "^4.26.1",
    "@tanstack/react-table": "^8.7.9",
    "axios": "~0.27.2",
    "clsx": "^1.2.1",
    "qs": "^6.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-router-dom": "^6.9.0",
    "react-transition-group": "^4.4.5",
    "zod": "^3.21.4",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^4.26.2",
    "@types/node": "^18.15.2",
    "@types/qs": "^6.9.7",
